<launch>
    
    <node pkg="stargazer" name="stargazer_publisher"  type="stargazer_publisher.py" output="screen">
    	   <rosparam command="load" file="$(find stargazer)/config/stargazer.yaml"/>
    	   <rosparam command="load" file="$(find stargazer)/config/stargazer_init.yaml"/>
    	   <rosparam command="load" file="$(find stargazer)/config/map.yaml"/>
    </node>

    <node pkg="tf" type="static_transform_publisher" name="base_link_broadcaster" args="0 0 0 0 0 0 1 map base_link 100" />

    <node pkg="tf" type="static_transform_publisher" name="stargazer_broadcaster" args="0 0 0.30 0 0 0 1 base_link  stargazer 100" />
</launch>
